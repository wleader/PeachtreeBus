        <file src=".\PeachtreeBus\bin\Release\netstandard2.1\PeachtreeBus.Interfaces.dll" target="lib\netstandard2.1\PeachtreeBus.Interfaces.dll" />
        <file src=".\PeachtreeBus\bin\Release\netstandard2.1\PeachtreeBus.Interfaces.pdb" target="lib\netstandard2.1\PeachtreeBus.Interfaces.pdb" />
